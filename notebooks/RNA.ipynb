   "source": [
    "# Transcribing DNA into RNA\n",
    "\n",
    "https://rosalind.info/problems/rna/"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "00f56db8-1321-437c-9f40-2361b3c0f693",
   "metadata": {},
   "outputs": [],
   "source": [
    "def dna_to_rna(dna):\n",
    "    return dna.replace(\"T\", \"U\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "e345ae98-b2ae-4fcd-b190-2cd226376197",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "\n"
     ]
    }
   ],
   "source": [
    "with open(\"../data/rosalind_rna.txt\", \"r\") as reader:\n",
    "    dna_string = reader.read()\n",
    "\n",
    "result = dna_to_rna(dna_string)\n",
    "print(result)"
   ]
  }
